var app = angular.module("hikehk",['ngRoute', 'ui.bootstrap', 'uiGmapgoogle-maps']);

app.config(['$routeProvider', 'uiGmapGoogleMapApiProvider', function($routeProvider, uiGmapGoogleMapApiProvider){
  $routeProvider.when('/', {
    templateUrl: "<%= asset_path('pages/home.html') %>",
    controller: 'HomeCtrl'
  });

  uiGmapGoogleMapApiProvider.configure({
      key: "<%= ENV['CASSIE_GOOGLEKEY'] %>",
      v: '3.17',
      libraries: 'weather,geometry,visualization'
  });

}]);

